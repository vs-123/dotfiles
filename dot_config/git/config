   cmd = nvim -d "$LOCAL" "$REMOTE"

[alias]
   a = add
   aa = add -A
   b = branch
   bm = branch -M
   bd = branch -D
   c = commit
   ca = commit --all
   cam = commit --amend
   cfg = config
   ch = checkout
   cl = clone
   d = diff
   dt = difftool -y
   f = fetch
   gcfg = config --global
   l = log --reverse
   lr = log 
   pl = pull
   pl = pull --rebase
   ps = push
   psu = push -u
   psf = push -f
   rb = rebase --committer-date-is-author-date
   rbi = rebase -i --committer-date-is-author-date
   rbir = rebase -i --root --committer-date-is-author-date
   rem = remote
   rema = remote add
   remr = remote remove
   rmc = rm --cache
   rs = reset
   rsh = reset --hard
   st = status
   sw = switch
